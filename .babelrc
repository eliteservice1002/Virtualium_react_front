{
	"presets": [
		"@babel/preset-react",
		["@babel/preset-env", {
			"useBuiltIns": "usage",
			"corejs": "3",
			"targets": {
				"browsers": [
					"last 5 versions",
					"ie >= 8"
				]
			}
		}]
	],
	"plugins": [
		"react-css-modules",
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-class-properties"
	]
}